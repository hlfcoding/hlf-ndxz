<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>{{title}} {pengxwang}</title>
    <meta name="verify-v1" content="{{config.google.webmaster_key}}">
    <meta name="profile" content="http://gmpg.org/xfn/11">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta name="build" content="{{config.theme.build}}">
  {{#config}}
    {{#meta.owner}}
    <meta name="description" content="{{description}}">
    <meta name="keywords" content="{{keywords}}">
    <meta name="author" content="{{name}}">
    <meta name="owner" content="{{email}}">
    {{/meta.owner}}
    {{#theme.copyright}}
    <link rel="license content-license" href="{{license_url}}">
    <link rel="license source-license" href="{{source_url}}">
    {{/theme.copyright}}
  {{/config}}
    <link rel="shortcut icon" href="/theme/favicon.ico">
    <link rel="apple-touch-icon" href="/theme/favicon.png">
    <link rel="stylesheet" href="/lib/theme/hlf-dom-extensions.css">
    <link rel="stylesheet" href="/theme/style.css">
  </head>
  <body>
    <div id="index" class="index">
      {{>index}}
    </div>
    <div id="post" role="main" class="post">
      {{{post}}}
    </div>
  {{#debug_info}}
    <script>Silexhibit.debugInfo = {{{debug_info}}};</script>
  {{/debug_info}}
    <script src="/lib/theme/hlf-dom-extensions.js"></script>
    <script src="/theme/extend.js"></script>
    <script>
      var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
      document.write(unescape("%3Cscript src=\'" + gaJsHost + "google-analytics.com/ga.js\' type=\'text/javascript\'%3E%3C/script%3E"));
      try {
        var pageTracker = _gat._getTracker("{{config.google.analytics_tracker}}");
        pageTracker._trackPageview();
      } catch (err) {}
    </script>
  </body>
</html>
