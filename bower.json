{
  "name": "silexhibit",
  "version": "0.0.1",
  "dependencies": {
    "almond": "~0.2",
    "backbone": "~1.1",
    "blueimp-canvas-to-blob": "~2.1",
    "blueimp-file-upload": "~9.5",
    "blueimp-load-image": "~1.11",
    "blueimp-tmpl": "~2.5.3",
    "bourbon": "~3.1",
    "codemirror": "~3.19",
    "jquery": "~1.10",
    "modernizr": "~2.6",
    "moment": "~2.3",
    "mustache": "~0.7",
    "spectrum": "~1.2",
    "underscore": "~1.5",
    "underscore.string": "~2.3",
    "flex-slider": "git@github.com:woothemes/FlexSlider.git#version/2.2",
    "foundation": "git@github.com:zurb/foundation.git#scss-standalone",
    "javascript-debug": "git@github.com:GerHobbelt/javascript-debug.git#master",
    "underscore.inflection": "git@github.com:jeremyruppel/underscore.inflection.git#master",
    "vimeo-player-api": "git@github.com:hlfcoding/player-api.git",
    "x2js": "git@github.com:abdmob/x2js.git#master",
    "hlf-css": "git@github.com:hlfcoding/hlf-css.git#latest-stable",
    "hlf-jquery": "git@github.com:hlfcoding/hlf-jquery.git#latest-stable"
  },
  "exportsOverride": {
    "almond": {
      "js/optional": "almond.js"
    },
    "backbone": {
      "js/optional": "backbone.js"
    },
    "blueimp-canvas-to-blob": {
      "js/optional/jquery.fileupload": "js/canvas-to-blob.js"
    },
    "blueimp-file-upload": {
      "css/optional": "css/jquery.fileupload.css",
      "js/optional/jquery.fileupload": "js/{vendor/*,jquery.iframe-transport.js,jquery.fileupload{,-process,-image,-validate}.js}"
    },
    "blueimp-load-image": {
      "js/optional/jquery.fileupload": "js/{load-image,load-image-*}.js"
    },
    "blueimp-tmpl": {},
    "bourbon": {
      "scss/optional/bourbon": "app/assets/stylesheets"
    },
    "codemirror": {
      "css/optional": "lib/codemirror.css",
      "js/optional": "lib/codemirror.js",
      "js/optional/codemirror-modes": "mode/{htmlmixed/htmlmixed,xml/xml,javascript/javascript,css/css}.js"
    },
    "foundation": {
      "js/optional/foundation": "js/foundation/*",
      "scss/optional/foundation": "scss/foundation/*"
    },
    "javascript-debug": {
      "js/development": "ba-debug.js"
    },
    "jquery": {
      "js": "jquery.js"
    },
    "modernizr": {
      "js/head": "modernizr.js"
    },
    "moment": {
      "js/optional": "moment.js"
    },
    "mustache": {
      "js/optional": "mustache.js"
    },
    "spectrum": {
      "css/optional": "spectrum.css",
      "js/optional": "spectrum.js"
    },
    "underscore": {
      "js": "underscore.js"
    },
    "underscore.string": {
      "js/optional": "lib/underscore.string.js"
    },
    "flex-slider": {
      "js/optional": "jquery.flexslider.js"
    },
    "underscore.inflection": {
      "js/optional": "src/underscore.inflection.js"
    },
    "vimeo-player-api": {
      "js/optional": "javascript/froogaloop.js"
    },
    "x2js": {
      "js/optional": "xml2json.js"
    },
    "hlf-css": {
      "font": "font/{fontawesome-webfont,FontAwesome}.*",
      "scss": "{normalize,hlf.{mixins,boilerplate-mod,fontawesome-mod,ui.grid,ui.menu}}.scss"
    },
    "hlf-jquery": {
      "coffee": "src/jquery.extension.hlf.{core,event}.coffee",
      "coffee/optional": "src/jquery.hlf.tip.coffee",
      "scss/optional": "release/jquery.hlf.tip.scss"
    }
  }
}
