<a class="permalink compound" href="{{link}}" rel="bookmark"
   target="_blank" data-tooltip="">
  <article>
    <span class="title faux-link">{{{title}}}</span>
    <span class="delim">|</span>
    <time datetime="{{date.pub}}" pubdate>{{date.human}}</time>
    <span class="delim">|</span>
    <abbr class="stat" title="{{note_count}} notes">{{note_count}}</abbr>
    <span class="delim">|</span>
    <abbr class="truncated tags" title="{{tags}}">{{tags}}</abbr>
  </article>
</a>
