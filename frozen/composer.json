  "version": "0.0.1",
  "type": "project",
  "keywords": [],
  "homepage": "http://pengxwang.com/open-source/silexhibit",
  "time": "2014-01-01",
  "license": "MIT",
  "authors": [
    {
      "name": "Peng Wang",
      "email": "peng@pengxwang.com",
      "homepage": "http://pengxwang.com",
      "role": "Developer"
    }
  ],
  "require": {
    "silex/silex": "~1.0",
    "doctrine/dbal": "~2.0",
    "guzzle/guzzle": "~3.0",
    "guzzle/silex-provider": "~1.0",
    "igorw/config-service-provider": "~1.0", "symfony/yaml": "~2.0",
    "kriswallsmith/assetic": "dev-master",
    "monolog/monolog": "~1.0", "symfony/monolog-bridge": "~2.0",
